############### Launch SSD ################


# SSD Trained SatNet, works.
source ~/.profile
conda activate tf112-gpu
cd ~/research/SSD_Keras
python train_and_test.py --gpu_list="0" --train_tfrecord=/home/jfletcher/data/satnet/tfrecords/SatNet.v.1.0.0.0/unfiltered_single_frame/satnet_full_singleframes_v1_0_0_train.tfrecords --valid_tfrecord=/home/jfletcher/data/satnet/tfrecords/SatNet.v.1.0.0.0/unfiltered_single_frame/satnet_full_singleframes_v1_0_0_valid.tfrecords --test_tfrecord=/home/jfletcher/data/satnet/tfrecords/SatNet.v.1.0.0.0/unfiltered_single_frame/satnet_full_singleframes_v1_0_0_test.tfrecords --num_train_images=18000 --num_valid_images=6000 num_test_images=6000 --batch_size=28 --num_training_epochs=100000 --use_tensorboard --run_name="ssd_on_satnet_0" --metadata_path=/opt/tfrecords/SatNet.v.1.0.0.0/SatNet/data



# SSD Trained SatSim, sensor bank.
